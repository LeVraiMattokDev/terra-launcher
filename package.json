{
    "name": "terralauncher",
    "preductname": "Terra Launcher",
    "version": "1.0.8",
    "description": "Launcher Minecraft pour le serveur Terra",
    "main": "src/app.js",
    "author": {
        "name": "Mattok, Terra, Inc.",
        "email": "contact@mattok.ch",
        "url": "https://github.com/"
    },
    "url": "http://launcher.mattok.ch",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=dev electron .",
        "dev": "cross-env-shell DEV_TOOL=open nodemon --exec npm start",
        "build": "node build.js --obf=true --build=platform",
        "icon": "node build.js --icon=https://terra-survie.wstr.fr/storage/img/terralogo.png"
    },
    "license": "CCANC",
    "nodemonConfig": {
        "ext": "js,html,css",
        "ignore": "**/test"
    },
    "dependencies": {
